{
  "name": "jsdoc-http-plugin",
  "version": "0.2.3",
  "description": "Plugin to add HTTP Route tags for documenting HTTP endpoints",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "clean": "rm -rf ./out/*;",
    "example1": "rm -rf ./out/*; ./node_modules/.bin/jsdoc -c ./examples/jsdoc.conf --readme ./examples/README.md; true"
  },
  "keywords": [
    "jsdoc",
    "documentation",
    "express",
    "restful",
    "rest",
    "route",
    "plugin"
  ],
  "engines": {
    "node": ">=4.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vmarchaud/jsdoc-http-plugin.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Brad van der Laan",
  "license": "MIT",
  "dependencies": {
    "jsdoc": "^3.4.3"
  },
  "peerDependencies": {
    "jsdoc": "^3.4.3"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "cheerio": "^1.0.0-rc.2"
  },
  "ava": {
    "files": [
      "spec/**/*.spec.js"
    ]
  }
}
